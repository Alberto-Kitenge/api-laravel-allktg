# ============================================
# Laravel .gitignore
# ============================================

# ----------------
# Environnement
# ----------------
.env
.env.backup
.env.production
.env.local
.env.*.local

# ----------------
# Dépendances
# ----------------
/vendor
/node_modules
composer.lock
package-lock.json
yarn.lock
pnpm-lock.yaml

# ----------------
# Laravel
# ----------------
/storage/*.key
/storage/pail
/storage/logs/*.log
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
!/storage/framework/cache/.gitkeep
!/storage/framework/sessions/.gitkeep
!/storage/framework/testing/.gitkeep
!/storage/framework/views/.gitkeep
/bootstrap/cache/*
!/bootstrap/cache/.gitkeep

# Public assets
/public/build
/public/hot
/public/storage
/public/mix-manifest.json
/public/css
/public/js

# ----------------
# Tests & Cache
# ----------------
.phpunit.result.cache
/.phpunit.cache
coverage/
*.coverage
clover.xml

# ----------------
# IDE & Éditeurs
# ----------------
# PhpStorm / IntelliJ
/.idea
*.iml
*.iws
*.ipr
.idea_modules/

# VSCode
/.vscode
*.code-workspace
.history/

# Sublime Text
*.sublime-project
*.sublime-workspace

# Nova
/.nova

# Fleet
/.fleet

# Zed
/.zed

# NetBeans
nbproject/

# Eclipse
.project
.buildpath
.settings/

# ----------------
# Outils PHP
# ----------------
.phpactor.json
.php_cs.cache
.php-cs-fixer.cache
.phpcs-cache
.psalm/
.phpstan/
/auth.json

# ----------------
# Système d'exploitation
# ----------------
# macOS
.DS_Store
.AppleDouble
.LSOverride
._*

# Windows
Thumbs.db
Thumbs.db:encryptable
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/

# Linux
*~
.directory
.Trash-*

# ----------------
# Homestead & Docker
# ----------------
Homestead.json
Homestead.yaml
.vagrant/
docker-compose.override.yml

# ----------------
# Logs
# ----------------
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# ----------------
# Temporaires
# ----------------
*.tmp
*.temp
*.swp
*.swo
*.bak
*.cache
*.pid
*.seed
*.pid.lock

# ----------------
# Données sensibles
# ----------------
*.pem
*.key
*.cert
*.crt
*.p12
*.pfx

# ----------------
# Divers
# ----------------
/.phpintel
/.vscode-server
/_ide_helper.php
/_ide_helper_models.php
.phpstorm.meta.php
